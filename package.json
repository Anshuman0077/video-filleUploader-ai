{
  "name": "video-file-uploader-root",
  "version": "1.0.0",
  "description": "Root package for the video file uploader application",
  "private": true,
  "scripts": {
    "install:all": "npm install && npm install --prefix backend && npm install --prefix frontend",
    "dev:services": "docker-compose up -d",
    "stop:services": "docker-compose down",
    "dev:backend": "npm run dev --prefix backend",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm:dev:services\" \"npm:dev:backend\" \"npm:dev:frontend\"",
    "postinstall": "npm run install:all"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "axios": "^1.12.2",
    "express-rate-limit": "^8.1.0",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.4",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
